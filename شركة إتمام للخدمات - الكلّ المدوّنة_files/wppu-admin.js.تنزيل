            $(function () {
                $('#WAButton').floatingWhatsApp({
                    phone: "+966533030283", //WhatsApp Business phone number
                    headerTitle: "أهلاً بك في إتمام", //Popup Title
                    popupMessage: `يرجى منك التّكرّم بشرح كيف نستطيع أن نخدمك؟<br />
