(function(global){var Language = {};

Language.pluralFormFunction = function(n){
		var mod100 = n % 100;

		if (n === 0) {
			return 'zero';
		}

		if (n === 1) {
			return 'one';
		}

		if (n === 2) {
			return 'two';
		}

		if (mod100 >= 3 && mod100 <= 10) {
			return 'few';
		}

		if (mod100 >= 11 && mod100 <= 99) {
			return 'many';
		}

		return 'other';
	};


